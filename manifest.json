{
  "manifest_version": 3,
  "name": "fAIshion Virtual Try-On Plugin",
  "description": "Virtual Try-On on right-click",
  "version": "1.0.6",
  "permissions": [
    "contextMenus",
    "storage",
    "activeTab",
    "scripting"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "resources/bg.png"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}